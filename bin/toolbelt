#!/usr/bin/env php
<?php

require __DIR__.'/../bootstrap/app.php';

$kernel = \FondBot\Toolbelt\Factory::create(
    $container,
    __DIR__.'/..',
    __DIR__.'/../resources/fondbot'
);

/** @var Symfony\Component\Console\Application $console */
$console = $kernel->resolve('console');

$console->run();
